#!/bin/bash

if [[ -e /Library/LaunchDaemons/codes.aurora.kayokod.plist ]]; then
    launchctl load -w /Library/LaunchDaemons/codes.aurora.kayokod.plist > /dev/null 2>&1
elif [[ -e /var/jb/Library/LaunchDaemons/codes.aurora.kayokod.plist ]]; then
    launchctl load -w /var/jb/Library/LaunchDaemons/codes.aurora.kayokod.plist > /dev/null 2>&1
fi
